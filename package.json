{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx ."
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "organize-imports-cli",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@types/fs-extra": "^9.0.6",
    "@types/node-fetch": "^2.5.7",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "date-fns": "^2.16.1",
    "eslint": "^7.18.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "gray-matter": "^4.0.2",
    "next": "^10.0.5",
    "node-fetch": "^2.6.1",
    "organize-imports-cli": "^0.8.0",
    "postcss": "^8.2.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "rss-parser": "^3.10.0",
    "ss-parser": "^1.0.3",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
